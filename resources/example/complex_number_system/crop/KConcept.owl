        <rdfs:domain rdf:resource="http://www.cs.teilar.gr/ontologies/KConcept.owl#EducationalObjective"/>
        <rdfs:range rdf:resource="http://www.cs.teilar.gr/ontologies/KConcept.owl#EducationalObjective"/>
