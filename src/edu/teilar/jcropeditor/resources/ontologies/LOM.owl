    <!-- http://www.cs.teilar.gr/ontologies/LOM.owl#hasElement -->

    <owl:ObjectProperty rdf:about="http://www.cs.teilar.gr/ontologies/LOM.owl#hasElement">
        <rdfs:range rdf:resource="http://www.cs.teilar.gr/ontologies/LOM.owl#Element"/>
        <isPartOfValuePartition rdf:resource="http://www.cs.teilar.gr/ontologies/LOM.owl#Element"/>
        <rdfs:domain rdf:resource="http://www.cs.teilar.gr/ontologies/LOM.owl#LOM"/>
        <owl:inverseOf rdf:resource="http://www.cs.teilar.gr/ontologies/LOM.owl#isElementOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cs.teilar.gr/ontologies/LOM.owl#hasElementComponent -->

    <owl:ObjectProperty rdf:about="http://www.cs.teilar.gr/ontologies/LOM.owl#hasElementComponent">
        <isPartOfValuePartition rdf:resource="http://www.cs.teilar.gr/ontologies/LOM.owl#ElementComponent"/>
        <rdfs:range rdf:resource="http://www.cs.teilar.gr/ontologies/LOM.owl#ElementComponent"/>
        <owl:inverseOf rdf:resource="http://www.cs.teilar.gr/ontologies/LOM.owl#isElementComponentOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.cs.teilar.gr/ontologies/LOM.owl#Classification"/>
                    <rdf:Description rdf:about="http://www.cs.teilar.gr/ontologies/LOM.owl#General"/>
                    <rdf:Description rdf:about="http://www.cs.teilar.gr/ontologies/LOM.owl#LifeCycle"/>
                    <rdf:Description rdf:about="http://www.cs.teilar.gr/ontologies/LOM.owl#MetaMetaData"/>
                    <rdf:Description rdf:about="http://www.cs.teilar.gr/ontologies/LOM.owl#Relation"/>
                    <rdf:Description rdf:about="http://www.cs.teilar.gr/ontologies/LOM.owl#Requirement"/>
                    <rdf:Description rdf:about="http://www.cs.teilar.gr/ontologies/LOM.owl#Resource"/>
                    <rdf:Description rdf:about="http://www.cs.teilar.gr/ontologies/LOM.owl#TaxonPath"/>
                    <rdf:Description rdf:about="http://www.cs.teilar.gr/ontologies/LOM.owl#Technical"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.cs.teilar.gr/ontologies/LOM.owl#isElementComponentOf -->

    <owl:ObjectProperty rdf:about="http://www.cs.teilar.gr/ontologies/LOM.owl#isElementComponentOf">
        <rdfs:domain rdf:resource="http://www.cs.teilar.gr/ontologies/LOM.owl#ElementComponent"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
