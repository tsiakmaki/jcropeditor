    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#describes -->

    <owl:ObjectProperty rdf:about="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#describes">
        <rdfs:domain rdf:resource="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#LOM"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#hasElement -->

    <owl:ObjectProperty rdf:about="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#hasElement">
        <rdfs:domain rdf:resource="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#LOM"/>
        <rdfs:range rdf:resource="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#LOMElement"/>
        <owl:inverseOf rdf:resource="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#isElementOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#hasElementComponent -->

    <owl:ObjectProperty rdf:about="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#hasElementComponent">
        <rdfs:range rdf:resource="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#LOMElementComponent"/>
        <owl:inverseOf rdf:resource="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#isElementComponentOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#LOMElement"/>
                    <rdf:Description rdf:about="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#LOMElementComponent"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#isElementComponentOf -->

    <owl:ObjectProperty rdf:about="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#isElementComponentOf">
        <rdfs:domain rdf:resource="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#LOMElementComponent"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#isElementOf -->

    <owl:ObjectProperty rdf:about="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#isElementOf">
        <rdfs:range rdf:resource="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#LOM"/>
        <rdfs:domain rdf:resource="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#LOMElement"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#aggregationLevel -->

    <owl:DatatypeProperty rdf:about="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#aggregationLevel">
        <rdfs:comment>The functional granularity of this learning object.</rdfs:comment>
        <rdfs:comment>1: the smallest level
of aggregation, e.g.,
raw media data or
fragments.
2: a collection of
level 1 learning
objects, e.g., a lesson.
3: a collection of
level 2 learning
objects, e.g., a course.
4: the largest level of
granularity, e.g., a set
of courses that lead to
a certificate.
NOTE 1:--Level 4
objects can contain level
3 objects, or can
recursively contain
other level 4 objects.</rdfs:comment>
        <owl:propertyDisjointWith rdf:resource="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#coverage"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first rdf:datatype="&xsd;int">1</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first rdf:datatype="&xsd;int">2</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="&rdf;List"/>
                                        <rdf:first rdf:datatype="&xsd;int">3</rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="&rdf;List"/>
                                                <rdf:first rdf:datatype="&xsd;int">4</rdf:first>
                                                <rdf:rest rdf:resource="&rdf;nil"/>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#catalog -->

    <owl:DatatypeProperty rdf:about="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#catalog">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment>value space: Repetoire of ISO/IEC 10646-1:2000
(e.g. ISDN, URI etc)</rdfs:comment>
        <rdfs:comment>The name or designator of the identification or cataloguing scheme for this entry.  A namespace scheme.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#Identifier"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#context -->

    <owl:DatatypeProperty rdf:about="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#context">
        <rdfs:comment>The principal environment within which the learning and use of this learning object is intended to take place.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#copyrightAndOtherRestrictions -->

    <owl:DatatypeProperty rdf:about="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#copyrightAndOtherRestrictions">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment>Whether copyright or other restrictions apply to the use of this learning object.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#Rights"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first>no</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first>yes</rdf:first>
                                <rdf:rest rdf:resource="&rdf;nil"/>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#cost -->

    <owl:DatatypeProperty rdf:about="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#cost">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment>Whether use of this learning object requires payment.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#Rights"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first>no</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first>yes</rdf:first>
                                <rdf:rest rdf:resource="&rdf;nil"/>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#coverage -->

    <owl:DatatypeProperty rdf:about="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#coverage">
        <rdfs:comment>Refer also to Dublin Core Coverage element
http://www.dublincore.org/documents/dces/</rdfs:comment>
        <rdfs:comment>The time, culture, geography or region to which this learning object applies.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#General"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#date -->

    <owl:DatatypeProperty rdf:about="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#date">
        <rdfs:comment>The date of contribution or annotation</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#description -->

    <owl:DatatypeProperty rdf:about="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#description">
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#difficulty -->

    <owl:DatatypeProperty rdf:about="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#difficulty">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment>This data element defines how hard it is to work through this learning object for the typical target audience.</rdfs:comment>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first>difficult</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first>easy</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="&rdf;List"/>
                                        <rdf:first>medium</rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="&rdf;List"/>
                                                <rdf:first>very difficult</rdf:first>
                                                <rdf:rest>
                                                    <rdf:Description>
                                                        <rdf:type rdf:resource="&rdf;List"/>
                                                        <rdf:first>very easy</rdf:first>
                                                        <rdf:rest rdf:resource="&rdf;nil"/>
                                                    </rdf:Description>
                                                </rdf:rest>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#duration -->

    <owl:DatatypeProperty rdf:about="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#duration">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment>Time a continuous learning object takes when played at intended speed.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#Technical"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#entity -->

    <owl:DatatypeProperty rdf:about="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#entity">
        <rdfs:comment>recommended practice for values of this property
vCard, as defined by
IMC vCard 3.0 (RFC 2425, RFC 2426).</rdfs:comment>
        <rdfs:comment>The identification of and information about  people or organizations contributing to this learning object, most relevant first.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#entry -->

    <owl:DatatypeProperty rdf:about="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#entry">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#Identifier"/>
                    <rdf:Description rdf:about="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#Taxon"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#format -->

    <owl:DatatypeProperty rdf:about="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#format">
        <rdfs:comment>Technical datatype(s) of (all the components of) this learning object.

This data element shall be used to identify the software needed to access the learning object.</rdfs:comment>
        <rdfs:comment>CharacterString.   MIME types based on IANA registration (see RFC2048:1996) or &#39;non-digital&#39;
&quot;video/mpeg&quot;, &quot;application/x-toolbook&quot;, &quot;text/html&quot;</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#Technical"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#id -->

    <owl:DatatypeProperty rdf:about="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#id">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment>The identifier of the taxon, such as a number or letter combination provided by the source of the taxonomy.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#Taxon"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#installationRemarks -->

    <owl:DatatypeProperty rdf:about="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#installationRemarks">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment>Description of how to install this learning object.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#Technical"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#intendedEndUserRole -->

    <owl:DatatypeProperty rdf:about="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#intendedEndUserRole">
        <rdfs:comment>Principal user(s) for which this learning object was designed, most dominant first.</rdfs:comment>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first>author</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first>learner</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="&rdf;List"/>
                                        <rdf:first>manager</rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="&rdf;List"/>
                                                <rdf:first>teacher</rdf:first>
                                                <rdf:rest rdf:resource="&rdf;nil"/>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#interactivityLevel -->

    <owl:DatatypeProperty rdf:about="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#interactivityLevel">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment>The degree of interactivity characterising this learning object. Interactivity in this context refers to the degree to which the learner can influence the aspect or behaviour of the learning object.</rdfs:comment>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first>high</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first>low</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="&rdf;List"/>
                                        <rdf:first>medium</rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="&rdf;List"/>
                                                <rdf:first>very high</rdf:first>
                                                <rdf:rest>
                                                    <rdf:Description>
                                                        <rdf:type rdf:resource="&rdf;List"/>
                                                        <rdf:first>very low</rdf:first>
                                                        <rdf:rest rdf:resource="&rdf;nil"/>
                                                    </rdf:Description>
                                                </rdf:rest>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#interactivityType -->

    <owl:DatatypeProperty rdf:about="http://www.cs.teilar.gr/ontologies/LearningObjectMetaData.owl#interactivityType">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment>Predominant mode of learning supported by this learning object</rdfs:comment>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first>active</rdf:first>
                        <rdf:rest>
